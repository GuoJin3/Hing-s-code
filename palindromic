#include<stdio.h>

int isPalindromic(long long n);

int main()
{
	long long n;
	scanf("%lld",&n);
	if(isPalindromic(n))
		printf("true\n");
	else
		printf("false\n");
	return 0;
 } 
 
int isPalindromic(long long n)
{
	long long m=0;
	int flag=0;

	if(n>=0)
	{
		while(10*m<n)
		{
			m=10*m+n%10;
			n=n/10;
		}
		if(m==n)
			flag= 1;
		else
			if(m/10==n)
				flag= 1;	
	}
	return flag;
}
